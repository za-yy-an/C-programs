#include <stdio.h>

int main() {
    int choice;
    printf("1.Add  2.Subtract  3.Multiply\nEnter: ");
    scanf("%d", &choice);

    switch (choice) {
        case 1: printf("You chose Addition\n"); 
        break;
        case 2: printf("You chose Subtraction\n"); 
        break;
        case 3: printf("You chose Multiplication\n"); 
        break;
        default: printf("Invalid choice!\n");
    }

    return 0;
}
